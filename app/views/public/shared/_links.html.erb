<%- if controller_name != 'sessions' %>
  <div class="mb-2">
    <%= link_to "Log in", new_session_path(resource_name), class: "btn btn-primary btn-sm" %>
  </div>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <div class="mb-2">
    <%= link_to "Sign up", new_registration_path(resource_name), class: "btn btn-outline-primary btn-sm" %>
  </div>
<% end %>